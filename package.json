  "license": "MIT",
  "devDependencies": {
    "chai": "4.2.0",
    "mocha": "^8.1.3",
    "nodemon": "^2.0.4",
    "prettier": "2.1.2"
  },
  "dependencies": {
    "commander": "^6.1.0",
    "dot-prop": "^5.3.0",
    "fs-extra": "^9.0.1",
    "inflection": "^1.12.0",
    "lodash": "^4.17.20",
    "pkg-dir": "^4.2.0",
    "pkg-up": "^3.1.0",
    "rc": "^1.2.6",
    "scaffold-helper": "^1.2.0"
