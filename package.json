{
  "name": "@cliffpyles/blueprints-cli",
  "version": "1.10.0",
  "description": "A tool for generating predefined files and directories",
  "private": false,
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/cliffpyles/blueprints-cli"
  },
  "bin": {
    "bp": "./app/cli.js"
  },
  "scripts": {
